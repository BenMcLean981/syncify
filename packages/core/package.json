    "@types/node": "^22.12.0",
    "@types/sha1": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.19.0",
    "tsup": "^8.4.0",
    "typescript": "5.7.3",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "sha1": "^1.1.1",
    "uuid": "^11.0.5"
  }
}
