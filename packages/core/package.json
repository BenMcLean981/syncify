{
  "name": "@syncify/core",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist/**"
  ],
  "exports": {
